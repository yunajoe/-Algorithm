end_date= [0,31,28,31,30,31,30,31,31,30,31,30,31]

def func(m,d):
    for _ in range(len(end_date)): 
        if m > 12:
            return "BAD!"
        end_day = end_date[m]
        if d >=0 and d <=end_day:
            return "OK!"
        return "BAD!"
        
        
a,b = map(int, input().split())
func(a,b)


end_date= [0,31,28,31,30,31,30,31,31,30,31,30,31]

def func(m,d):
    if m > 12 or d > end_date[m] or d <0:
        print("BAD!")
    else:
        print("OK")
        
a, b =  map(int, input().split())
func(a,b)

