def func(n1,n2):    
    abs_n1, abs_n2 = abs(n1),abs(n2)
    max_num = max(abs_n1, abs_n2) 
    if max_num == abs_n1:
        return n1    
    return n2
        


def func2(n1,n2):
    abs_n1, abs_n2 = abs(n1),abs(n2)
    max_num = max(abs_n1, abs_n2) 
    if max_num == abs_n1:
        return "%.2f"% n2  
    return "%.2f"% n1   
        

a, b =  map(int, input().split())
func(a,b)

a, b =  map(float, input().split())
func2(a,b)


def func(n1,n2):
    if abs(n1) > abs(n2):
        print(n1)
    else:
        print(n2)
        
def func2(n1,n2):
    if abs(n1) > abs(n2):
        print("%.2f" % n2)
    else:
        print("%.2f" % n2)
        
a, b =  map(int, input().split())
func(a,b)

a, b =  map(float, input().split())
func2(a,b)

