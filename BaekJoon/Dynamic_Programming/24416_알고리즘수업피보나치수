def fib(n):
    if n == 1 or n == 2:
        return 1
    else:
        return fib(n-1) + fib(n-2)  
    
def fib2(n):
    answer = 0 
    box = [1,1]     
    for i in range(2,n):
        box.append(box[i-1] + box[i-2])
        answer += 1
    return answer

 
n = int(input())
print(fib(n), fib2(n))
