
for i in range(1,15):
    print(i,end=" ")
    
    
    
    
    
n = int(input()) 
i = 1
total = 0
while i <= n:
    total = total+i
    i = i + 1
print(total)


while True:
    num =int(input("number? "))
    if num == 0:
        break
    elif num > 0:
        print("positive integer")
    else:
        print("negative number")
    
    
    
total = 0
cnt = 0 
while True:
    num = int(input())
    cnt = cnt + 1
    total = total + num  
    avg = total / cnt 
    if num >=100:               
        print(total)
        print(round(avg,1))
        break

