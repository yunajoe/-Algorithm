def func(n):
    lis = []
    for i in range(n):
        num = int(input())
        lis.append(num)        
    num = sorted(lis, reverse=True)
    print(lis)
    
    
digit = int(input())
func(digit)


def func(lis):
    lis.sort(reverse=True)
    for num in lis:
        print(num, end=" ")
            
n = int(input())
num_lis = list(map(int, input().split()))
func(num_lis)

