res = []

def func(n,m):
    if len(res) == m:
        test = res[:]
        test.sort()
        if (test == res):
            for ele in res:
                print(ele, end= " ")
            print()
     
    else:
        for i in range(1,n+1):
            if i not in res:
                res.append(i)
                func(n,m)
                res.pop()  
    



n, m = map(int, input().split()) 
func(n,m)
