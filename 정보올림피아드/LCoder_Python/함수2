import math 

def func(num):
    num = sorted(num_list)    
    max_num = math.ceil(num[2])
    min_num = math.floor(num[0])
    res_num = round(num[1])
    print(max_num,min_num,res_num)     
    
    
    
num_list = list(map(float,input().split())) 
func(num_list)

