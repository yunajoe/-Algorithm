result = []
def func(n,m):
    if len(result) == m:
        for i in result:
            print(i, end=" ")
        print()
        return 
    else:
        for i in range(1,n+1):
            if i not in result:
                result.append(i)
                func(n,m)
                result.pop()

            
    
n, m = map(int, input().split())
func(n,m)
