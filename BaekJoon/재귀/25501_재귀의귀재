cnt = 0 

def recursion(strs, start, end):
    global cnt
    cnt += 1
    if start >= end: return True
    elif strs[start] != strs[end]: return False
    else: return recursion(strs, start+1, end-1)
     
def isPal(strs): 
    if recursion(strs,0, len(strs)-1):
        return f"1 {cnt}"
    else:
        return f"0 {cnt}"  

for i in range(int(input())):
    answer = isPal(input())
    print(answer)


