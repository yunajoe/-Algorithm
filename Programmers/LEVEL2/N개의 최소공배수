# 최대공약수 구하기 함수   

def gcd(a, b): 
    gcd_num = 0
    for i in range(1,min(a,b)+1):
        if a %i == 0 and b % i == 0:
            if i > gcd_num:
                gcd_num = i 
    return gcd_num          
        
def lcm(a,b):
    return (a * b) // gcd(a,b)  

def solution(arr):
    while len(arr) > 1:
        a = arr.pop()
        b = arr.pop()
        arr.append(lcm(a, b))
    return int(arr[0])
            
 
        
