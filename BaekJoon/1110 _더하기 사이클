n = int(input())
init = n
new = 0
cnt = 0 

while True:
    n1 = n//10
    n2 = n%10 
    n3 = n1 + n2 
    new = 10*n2 + n3 %10
    cnt = cnt + 1
    n = new 
    if new == init:
        break
    
print(cnt)


-----------------------------------------------------


temp = num = int(input())
cnt = 0 
while True:
    
    num1 = temp // 10 
    num2 = temp % 10 
    digit_sum = num1 + num2 
    temp = int(str(num2) + str(digit_sum % 10))
    cnt = cnt + 1
    if temp == num:
        break
cnt
